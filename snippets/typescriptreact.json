{
	"log": {
		"prefix": ["log"],
		"body": "console.log(${1:$TM_SELECTED_TEXT});"
	},
	"react-component": {
		"prefix": ["component", "react-component"],
		"body": [
			"const ${1:$TM_SELECTED_TEXT} = (${2:$TM_SELECTED_TEXT}) => {",
			"\t${3:$TM_SELECTED_TEXT}",
			"}",
			"",
			"export default ${1}"
		],
		"description": "React functional component"
	},
	"const": {
		"prefix": ["const"],
		"body": [
			"const ${1:$TM_SELECTED_TEXT} = ${2:$TM_SELECTED_TEXT}"
		]
	},
	"let": {
		"prefix": ["let"],
		"body": [
			"let ${1:$TM_SELECTED_TEXT} = ${2:$TM_SELECTED_TEXT}"
		]
	},

	"useCallback": {
		"prefix": "useCallback",
		"body": ["useCallback((${1}) => {",
			"\t${2}",
			"}, [${3}])"
		],
		"description": "React useCallback() hook"
	},
	"useMemo": {
		"prefix": "useMemo",
		"body": ["useMemo(() => ${1:value}, [${2}])"],
		"description": "React useMemo() hook"
	},
	"useState": {
		"prefix": "useState",
		"body": ["useState<${1:type}>(${2:initialValue})"],
		"description": "React useState() hook"
	},
	"useEffect": {
		"prefix": "useEffect",
		"body": ["useEffect(() => {",
			"\t${1:doSomething}",
			"}, [${2:a}])"
		],
		"description": "React useEffect() hook"
	},

	"StyleSheet": {
		"prefix": "StyleSheet",
		"body": [
			"StyleSheet.create({",
			"\t${2:container}: {",
			"\t\t${3}",
			"\t},",
			"})"
		],
		"description": "Create React Native Styled Components StyleSheet"
	},

	"itBlock": {
		"prefix": "it",
		"body": [
			"it('Should $1', () => {",
			"\t$2",
			"})"
    		],
		"description": "Testing `it` block"
	},


